{
  "name": "cypress_project",
  "version": "1.0.0",
  "scripts": {
    "test": "cypress run",
    "test:headless": "cypress run --headless",
    "test:open": "cypress open",
    "test:ci": "cypress run --record --parallel --key YOUR_PROJECT_KEY",
    "test:report": "mochawesome-merge --reportDir cypress/reports/mocha-merge.json && mochawesome-report-generator cypress/reports/mocha-merge.json -o cypress/reports/mochawesome.html",
    "coverage": "nyc report --reporter=html"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.0.0",
    "@cypress/webpack-preprocessor-istanbul": "^2.0.0",
    "@cypress/react": "^6.0.0",
    "@cypress/react-unit-test": "^1.0.0",
    "@cypress/skip-test": "^1.1.0",
    "@cypress/vite-dev-server": "^2.0.0",
    "@cypress/webpack-preprocessor-istanbul": "^2.0.0",
    "@cypress/react": "^6.0.0",
    "@cypress/react-unit-test": "^1.0.0",
    "@cypress/skip-test": "^1.1.0",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/code-coverage": "^3.14.0",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^14.2.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "istanbul-lib-coverage": "^3.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "sonarqube-scanner": "^4.3.0"
  }
}
